{
    "export": "doctrine2-annotation",
    "zip": false,
    "dir": "src/Model/Db",
    "params": {
        "addGeneratorInfoAsComment": false,
        "backupExistingFile": false,
        "skipPluralNameChecking": false,
        "enhanceManyToManyDetection": true,
        "bundleNamespace": "XframeCMS",
        "entityNamespace": "Model\\Db",
        "repositoryNamespace": "XframeCMS\\Repository",
        "useAnnotationPrefix": "ORM\\",
        "skipGetterAndSetter": true,
        "extendsClass": "\\XframeCMS\\Model\\Db\\AbstractModel",
        "propertyTypehint": true,
        "useAutomaticRepository": true,
        "indentation": 4,
        "filename": "%entity%.%extension%",
        "quoteIdentifier": false
    }
}
